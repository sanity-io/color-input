{
  "name": "@sanity/color-input",
  "version": "3.0.0-v3-studio.10",
  "description": "Color input",
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run clean && plugin-kit verify-package --silent && pkg-utils",
    "build": "pkg-utils build",
    "watch": "pkg-utils watch",
    "link-watch": "plugin-kit link-watch",
    "prepublishOnly": "npm run build",
    "compile": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier src -w",
    "prepare": "husky install"
  },
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./lib/src/index.d.ts",
      "source": "./src/index.ts",
      "import": "./lib/index.esm.js",
      "require": "./lib/index.js",
      "default": "./lib/index.esm.js"
    }
  },
  "main": "./lib/index.js",
  "module": "./lib/index.esm.js",
  "types": "./lib/src/index.d.ts",
  "files": [
    "src",
    "lib",
    "v2-incompatible.js",
    "sanity.json"
  ],
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "color-input",
    "sanity-plugin"
  ],
  "dependencies": {
    "@sanity/icons": "1.3.6",
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/ui": "1.0.0-next.3",
    "lodash": "^4.17.21",
    "react-color": "^2.19.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@parcel/packager-ts": "^2.6.0",
    "@parcel/transformer-typescript-types": "^2.6.0",
    "@sanity/pkg-utils": "^1.16.2",
    "@sanity/plugin-kit": "^2.0.2",
    "@sanity/semantic-release-preset": "^2.0.2",
    "@types/react-color": "^2.17.6",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "parcel": "^2.6.0",
    "prettier": "^2.7.1",
    "react": "^18.0.0",
    "rimraf": "^3.0.2",
    "sanity": "3.0.0-rc.0",
    "semantic-release": "^19.0.3",
    "styled-components": "^5.2.0",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "sanity": "dev-preview || 3.0.0-rc.0",
    "styled-components": "^5.2.0"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/color-input/issues"
  },
  "homepage": "https://github.com/sanity-io/color-input#readme",
  "sanityExchangeUrl": "https://www.sanity.io/plugins/color-input",
  "repository": {
    "type": "git",
    "url": "git@github.com:sanity-io/color-input.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
