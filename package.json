{
  "name": "@sanity/color-input",
  "version": "3.0.0-v3-studio.5",
  "description": "Color input",
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run clean && plugin-kit verify-package --silent",
    "build": "parcel build --no-cache",
    "watch": "parcel watch",
    "link-watch": "plugin-kit link-watch",
    "prepublishOnly": "npm run build",
    "compile": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier src -w",
    "prepare": "husky install"
  },
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js",
      "default": "./lib/esm/index.js"
    }
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/index.d.ts",
  "files": [
    "src",
    "lib",
    "v2-incompatible.js",
    "sanity.json"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "color-input",
    "sanity-plugin"
  ],
  "dependencies": {
    "@sanity/icons": "^1.2.8",
    "@sanity/incompatible-plugin": "^1.0.1",
    "@sanity/ui": "^0.38.2",
    "lodash": "^4.17.21",
    "react-color": "^2.19.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@parcel/packager-ts": "^2.6.0",
    "@parcel/transformer-typescript-types": "^2.6.0",
    "@sanity/plugin-kit": "^1.1.0-ecosystem-preset.5",
    "@sanity/semantic-release-preset": "^2.0.1",
    "@types/react-color": "^2.17.6",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-sanity": "^5.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "parcel": "^2.6.0",
    "prettier": "^2.6.2",
    "react": "^17.0.0 || ^18.0.0",
    "rimraf": "^3.0.2",
    "sanity": "3.0.0-dev-preview.17",
    "semantic-release": "^19.0.3",
    "styled-components": "^5.2.0",
    "typescript": "4.7.4"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "sanity": "dev-preview",
    "styled-components": "^5.2.0"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/color-input/issues"
  },
  "homepage": "https://www.sanity.io/",
  "sanityExchangeUrl": "https://www.sanity.io/plugins/color-input",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/color-input.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
